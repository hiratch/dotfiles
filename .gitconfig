[color]
	ui = auto
[alias]
	st = status
	co = checkout
	push-f = push --force-with-lease
        graph = log --graph --all
[user]
	email = wanpac@gmail.com
	name = K.Hiramoto

[includeIf "gitdir:~/NMWork/"]
        path = ~/.gitconfig_nm

[diff]
	tool = meld
[credential]
	helper = osxkeychain
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[push]
	autoSetupRemote = true
